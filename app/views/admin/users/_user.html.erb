<tr>
  <td align="center">
    <%= link_to edit_admin_user_path(user),
    class: "btn btn-default" do %>
      <em class="fa fa-pencil"></em>
    <% end %>
    <%= link_to admin_user_path(user), method: :delete,
      class: "btn btn-danger", data: {confirm: "Xác nhận xóa?"} do %>
      <em class="fa fa-trash"></em>
    <% end %>
  </td>
  <td><%= user.ten %></td>
  <td><%= user.email %></td>
  <td><%= user.sdt %></td>
  <td><%= user.diachi %></td>
  <td><%= t "user_role.#{user.role}" %></td>
</tr>
